[alias]
	pff = push --force-with-lease
	rbm = !git fetch origin && git rebase origin/master
	rbc = !git add . && git rebase --continue
	rba = rebase --abort
	co = checkout
	cp = cherry-pick
	fo = fetch origin
	rsmaster = !git fetch origin && git reset --hard origin/master

